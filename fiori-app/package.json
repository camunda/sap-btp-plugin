{
  "name": "@camunda8/fiori-app",
  "version": "1.0.0-alpha.1",
  "description": "generic Fiori/UI5 app for Camunda BTP Integration",
  "license": "Apache-2.0",
  "author": "Volker Buzek",
  "scripts": {
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "clean": "rimraf dist coverage",
    "lint": "eslint webapp",
    "//start": "production start, serves /dist and thus need a prior build",
    "start": "node index.js",
    "//start:local": "dev-time only",
    "start:local": "ui5 serve --port 8080",
    "test": "npm run lint && npm run karma-ci-cov",
    "test:e2e": "run-p -r start:local \"wdi5 -- --headless --debug\"",
    "ts-typecheck": "tsc --noEmit",
    "wdi5": "wait-on tcp:8080 && wdio run ./webapp/test/e2e/wdio.conf.ts"
  },
  "dependencies": {
    "@sap/approuter": "^17.1.0",
    "@ui5/webcomponents": "^2.8.0",
    "@ui5/webcomponents-icons": "^2.8.0",
    "feelers": "^1.4.0",
    "ui5-cc-md": "^0.0.7"
  },
  "devDependencies": {
    "@camunda8/user-task-fiori": "file:../user-task-fiori",
    "@openui5/types": "^1.134.0",
    "@ui5/cli": "^4.0.14",
    "@wdio/cli": "^8",
    "@wdio/local-runner": "^8",
    "@wdio/mocha-framework": "^8",
    "@wdio/spec-reporter": "^8",
    "eslint": "^9.23.0",
    "globals": "^15.12.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "ui5-middleware-livereload": "^3.1.1",
    "ui5-tooling-modules": "^3.24.10",
    "ui5-tooling-transpile": "^3.7.5",
    "wait-on": "^8.0.3",
    "wdio-ui5-service": "^2.1.1"
  },
  "engines": {
    "node": "^22"
  }
}
