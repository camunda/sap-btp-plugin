<mvc:View xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    height="100%"
controllerName="io.camunda.connector.sap.btp.controller.Protocol">
    <ScrollContainer height="100%"
        horizontal="false"
        vertical="true">
        <List headerText="{i18n>protocolTitle}"
            noDataText="{i18n>history_empty}"
            items="{
                path: 'HistoryView>/qa'
            }">
            <items>
                <StandardListItem title="{HistoryView>question}"
                    description="{HistoryView>answer}" />
            </items>
        </List>
    </ScrollContainer>
</mvc:View>