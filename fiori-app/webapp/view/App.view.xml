<mvc:View controllerName="io.camunda.connector.sap.btp.controller.App"
	displayBlock="true"
	height="100%"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<App id="app">
		<Page showFooter="{= ${Messages>/}.length > 0 }">
			<customHeader>
				<Toolbar>
					<Image src="img/camunda.png"
						class="Camunda"
						height="40px"></Image>
					<ToolbarSpacer />
					<Title text="{i18n>AppView.title}" />
					<ToolbarSpacer />
					<Button icon="sap-icon://menu2"
						press="onGeneralMenuPress"
						ariaHasPopup="Menu" />
				</Toolbar>
			</customHeader>

			<!-- <f:FlexibleColumnLayout id="flexibleColumnLayout"
				layout="MidColumnFullScreen"
