<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
>
	<Dialog title="{i18n>ProcessDialog.pid}">
		<content>
			<Input
				id="processName"
				width="90%"
				class="sapUiTinyMargin"
				submit=".runThis"
			/>
		</content>
		<beginButton>
			<Button
				text="{i18n>ProcessDialog.start}"
				press=".runThis"
			/>
		</beginButton>
		<endButton>
			<Button
				text="{i18n>ProcessDialog.close}"
				press=".closeSRunThisProcessDialog"
			/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>