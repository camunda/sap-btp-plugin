{
	"_version": "1.12.0",

	"sap.app": {
		"id": "io.camunda.connector.sap.btp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${version}"
		},
		"dataSources": {
			"backend": {
				"uri": "/odata/v4/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "io.camunda.connector.sap.btp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.128.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "io.camunda.connector.sap.btp.i18n.i18n"
				}
			},
			"backend": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"dataSource": "backend",
				"settings": {
					"autoExpandSelect": true,
					"earlyRequests": true,
					"groupId": "$auto",
					"operationMode": "Server",
					"updateGroupId": "$auto"
				}
			}
		},

		"routing": {}
	}
}
