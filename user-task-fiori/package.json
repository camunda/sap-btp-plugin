{
	"name": "@camunda8/user-task-fiori",
	"version": "1.0.0-alpha.1",
	"description": "rendering Camunda Forms into the Fiori Design System",
	"license": "Apache-2.0",
	"author": "Volker Buzek",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "ui5 build --clean-dest",
		"build:ts-interfaces": "npx @ui5/ts-interface-generator",
		"clean": "rimraf dist coverage",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"lint": "eslint src test",
		"start": "ui5 serve -o test-resources/io/camunda/connector/sap/btp/lib/Example.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"test": "npm run lint && npm run karma-ci-cov",
		"testsuite": "ui5 serve -o test-resources/io/camunda/connector/sap/btp/lib/qunit/testsuite.qunit.html",
		"ts-typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@sapui5/types": "1.130.2",
		"@ui5/cli": "^4.0.10",
		"@ui5/ts-interface-generator": "^0.9.0",
		"eslint": "^9.15.0",
		"globals": "^15.12.0",
		"karma": "^6.4.4",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^4.1.0",
		"karma-ui5-transpile": "^3.5.1",
		"rimraf": "^6.0.1",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0",
		"ui5-middleware-livereload": "^3.1.0",
		"ui5-tooling-transpile": "^3.5.1"
	},
	"dependencies": {
    "feelers": "^1.4.0"
  }
}
